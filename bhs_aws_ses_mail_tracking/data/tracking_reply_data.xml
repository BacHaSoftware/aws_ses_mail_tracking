<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="bhs_aws_ses_mail_tracking.tracking_mailing" model="mailing.mailing">
            <field name="subject">To record reply mail messages</field>
        </record>

        <record id="bhs_aws_ses_mail_tracking.alias_domain" model="mail.alias.domain">
